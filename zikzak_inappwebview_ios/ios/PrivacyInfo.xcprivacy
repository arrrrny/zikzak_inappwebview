<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>NSPrivacyTracking</key>
    <false/>

    <key>NSPrivacyTrackingDomains</key>
    <array>
        <!-- Add any tracking domains here if needed -->
    </array>

    <key>NSPrivacyCollectedDataTypes</key>
    <array>
        <!-- WebView plugin doesn't collect user data directly -->
        <!-- Data collection is managed by the web content loaded -->
    </array>

    <key>NSPrivacyAccessedAPITypes</key>
    <array>
        <!-- UserDefaults API usage -->
        <dict>
            <key>NSPrivacyAccessedAPIType</key>
            <string>NSPrivacyAccessedAPICategoryUserDefaults</string>
            <key>NSPrivacyAccessedAPITypeReasons</key>
            <array>
                <!-- CA92.1: Access user defaults from app or app extension -->
                <string>CA92.1</string>
            </array>
        </dict>

        <!-- File timestamp API usage -->
        <dict>
            <key>NSPrivacyAccessedAPIType</key>
            <string>NSPrivacyAccessedAPICategoryFileTimestamp</string>
            <key>NSPrivacyAccessedAPITypeReasons</key>
            <array>
                <!-- C617.1: Access files or directories timestamps for app functionality -->
                <string>C617.1</string>
            </array>
        </dict>

        <!-- Disk space API usage -->
        <dict>
            <key>NSPrivacyAccessedAPIType</key>
            <string>NSPrivacyAccessedAPICategoryDiskSpace</string>
            <key>NSPrivacyAccessedAPITypeReasons</key>
            <array>
                <!-- E174.1: Display disk space to user -->
                <string>E174.1</string>
            </array>
        </dict>
    </array>
</dict>
</plist>
